{
    "name": "scones/resque-logger",
    "description": "resque-logger is a psr/log plugin for scones/resque to log all events",
    "type": "library",
    "homepage": "https://github.com/scones/resque-logger",
    "license": "MIT",
    "keywords": ["psr-12", "resque", "queue", "tasks", "task queue", "message queue"],
    "authors": [
        {
            "name": "Dirk Gustke",
            "email": "code+resque@asm68k.org",
            "homepage": "https://github.com/scones"
        }
    ],
    "require": {
        "php": "~7.2",
        "scones/resque": "*@stable",
        "psr/log": "*@stable"
    },
    "suggest": {
        "monolog/monolog": "*@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "*@stable"
    },
    "autoload": {
        "psr-4": {
            "ResqueLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ResqueLogger\\Tests\\": "tests/"
        }
    },
    "platform": {
        "php": "~7.2"
    }
}
