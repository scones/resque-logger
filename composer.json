{
    "name": "scones/resque-logger",
    "license": "github.com/scones/resque-logger/LICENSE",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/php-fig/event-dispatcher.git"
        }
    ],
    "require": {
        "php": "~7.2",
        "scones/resque": "*@stable",
        "psr/log": "*@stable"
    },
    "suggest": {
        "monolog/monolog": "*@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "*@stable"
    },
    "autoload": {
        "psr-4": {
            "ResqueLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ResqueLogger\\Tests\\": "tests/"
        }
    },
    "platform": {
        "php": "~7.2"
    }
}
